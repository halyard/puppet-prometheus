global:
  scrape_interval: 30s
  evaluation_interval: 30s

scrape_configs:
  - job_name: prometheus
    static_configs:
      - targets:
        - localhost:9090
  - job_name: node
    file_sd_configs:
      - files:
        - /etc/prometheus/servers_node.yml
  - job_name: wireguard
    file_sd_configs:
      - files:
        - /etc/prometheus/servers_wireguard.yml
