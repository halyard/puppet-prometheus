<% targets = scope.call_function('configvault_list', ['prometheus/blackbox/http']) -%>
<% targets.each do |target| -%>
<% urlstring = scope.call_function('configvault_read', ['prometheus/blackbox/http', true, 'FAIL', target]) -%>
<% urls = urlstring.split("\n") -%>
- targets:
<% urls.each do |url| -%>
    - <%= url %>
<% end -%>
  labels:
    nodename: <%= target %>
<% end -%>
<% end -%>
